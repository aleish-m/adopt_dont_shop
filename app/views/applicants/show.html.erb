<p><%= @applicant.first_name %> <%= @applicant.last_name %></p>
<p> <%="#{@applicant.street_address} #{@applicant.city}, #{@applicant.state} #{@applicant.zip}"%></p>
<p><%= @applicant.description %></p>
<div id="applicant-<%= @applicant.id %>" ><% @applicant.pets.each do |pet|%>
    <p><%= link_to "#{pet.name}", "/pets/#{pet.id}"%></p>
<% end %></div>
<%= @applicant.status%>
<section>
  <h1>Add a Pet to This Application</h1>

  <%= form_with url: "/applicants/#{@applicant.id}", method: :get, local: true do |f| %>
    <%= f.label "Search pet's name" %>
    <%= f.text_field :search %>
    <%= f.submit "Submit" %>
  <% end %>

</section>
 
